@model MojCRM.ViewModels.OrganizationDetailsViewModel
@{
    Layout = null;
}
<div class="col-md-3">

</div>
<div class="col-md-9">
    @if (Model.OrganizationBusinessUnits.Count() != 0)
    {
        <table class="table">
            <tr>
                <th>Naziv poslovnice</th>
            </tr>
            @foreach (var Unit in Model.OrganizationBusinessUnits)
            {
                <tr>
                    <td>@Unit.SubjectBusinessUnit</td>
                </tr>
            }
        </table>
    }
    else
    {
        <p>Ova tvrtka nema registriranih poslovnica</p>
    }
</div>
